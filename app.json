{
  "expo": {
    "name": "CyberChat",
    "slug": "CyberChat",
    "scheme": "CyberChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/darkIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/blueSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lightAdaptive.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kshprime.CyberChat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "6e267891-7f19-424c-b906-633d217bc0e8"
      }
    }
  }
}
